               </effect></Button>
            <Button fx:id="deleteAccountButton" mnemonicParsing="false" onMouseClicked="#onDeleteAccountClicked" prefHeight="65.0" prefWidth="420.0" stylesheets="@button.css" text="Delete Account" textFill="WHITE" GridPane.rowIndex="3">
