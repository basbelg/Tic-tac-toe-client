                  <Bloom />
               </effect></Button>
         </children>
      </GridPane>
      <Label alignment="CENTER" layoutY="25.0" prefHeight="59.0" prefWidth="441.0" style="-fx-background-color: #1f1f23;" styleClass="text" stylesheets="@label.css" text="Account" textFill="WHITE">
