      <Button fx:id="cancelButton" cancelButton="true" layoutX="281.0" layoutY="192.0" mnemonicParsing="false" onMouseClicked="#onCancelClicked" prefHeight="25.0" prefWidth="68.0" stylesheets="@button.css" text="Cancel" textFill="#ff8989">
