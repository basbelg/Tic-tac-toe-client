            <Bloom threshold="0.16" />
         </effect>
      </Label>
      <Label alignment="CENTER" layoutY="29.0" prefHeight="59.0" prefWidth="425.0" style="-fx-background-color: #1f1f23;" stylesheets="@label.css" text="Delete Account" textFill="#ff1e1e">
